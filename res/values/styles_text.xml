<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DashboardText">
        <item name="android:textColor">#22282A</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginBottom">20px</item>
    </style>
    <style name="BidInfoBaseText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#22282A</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="BidInfoLeftText" parent="BidInfoBaseText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dip</item>
    </style>
    <style name="BidInfoRightText" parent="BidInfoBaseText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">5dip</item>
    </style>
</resources>